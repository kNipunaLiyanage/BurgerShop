                    </div>
                    <!-- End opening time -->
                    <!-- Flickr !-->
                    <!-- End flickr -->
                </div>
            </div>
            <!-- End container -->
            <!-- Footer logo !-->
            <div class="footer_logo text-center">
                <img src="~/img/footer_logo.png" alt="logo">
                <p> 2021 ALL RIGHT RESERVED. DESIGNED BY <a target="_blank" href="">LMN Pvt Ltd</a></p>
            </div>
            <!-- End Footer logo !-->
        </footer>
        <a href="#" id="scroll_up"><i class="fa fa-angle-up"></i></a>
    </div>
</body>
</html>
