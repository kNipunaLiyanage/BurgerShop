<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    



    <link href="~/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/stylesheets/owl.carousel.css" rel="stylesheet" />
    <link href="~/stylesheets/owl.theme.css" rel="stylesheet" />
    <!-- Animate -->
    <link href="~/stylesheets/animate.css" rel="stylesheet" />
    <link href="~/stylesheets/jquery.datetimepicker.css" rel="stylesheet" />
    <!-- Pretty Photo -->

    <link href="~/stylesheets/prettyPhoto.css" rel="stylesheet" />
    <!-- Font awsome icons -->
    <link href="~/stylesheets/font-awesome.min.css" rel="stylesheet" />
    <link href="~/fonts/fonts.css" rel="stylesheet" />
    <!-- Main Template Styles -->
    <link href="~/stylesheets/main.css" rel="stylesheet" />
    <!-- Main Template Responsive Styles -->
    <link href="~/stylesheets/main-responsive.css" rel="stylesheet" />

</head>
<body>
    <div id="wrapper">
        <section class="banner dark">
            <div id="menu-single-parallax">
                <div class="bcg background42"
                     data-center="background-position: 50% 0px;"
                     data-bottom-top="background-position: 50% 100px;"
                     data-top-bottom="background-position: 50% -100px;"
                     data-anchor-target="#contact-parallax">
                    <div style="background-color=black;">
                        <div class="banner-content">
                            <div class="container">
                                <div class="slider-content">
                                    <i class="icon-home-ico"></i>
                                    <h1>Menu</h1>
                                    <p>Your Taste Is Our Goal</p>

                                </div>
                            </div>
                        </div>
                        <!-- End Banner content -->
                    </div>
                    <!-- End bg trnsparent -->
                </div>
            </div>
            <!-- Service parallax -->
        </section>
        <header id="header" class="header-transparent">
            <div class="container">
                <div class="row">
                    <div id="main-menu-trigger"><i class="fa fa-bars"></i></div>
                    <!-- Logo
                        ============================================= -->
                    <div id="logo"> <a href="" class="light-logo"><img src="~/img/logo.png" alt="Logo"></a> <a href="" class="dark-logo"><img src="~/img/logo_dark.png" alt="Logo"></a> </div>

                    <nav id="main-menu" class="dark">
                        
                        <ul>
                            <li>@Html.ActionLink("Home", "Index", "SinglemenuItem", new { @class = "nav-link" })</li>
                            <li>@Html.ActionLink("About us", "About", "SinglemenuItem", new { @class = "nav-link" })</li>
                            <li>@Html.ActionLink("Contact Us", "Contact", "SinglemenuItem", new { @class = "nav-link" })</li>

                            <li>@Html.ActionLink("Our Menu", "LoadMenu", "SinglemenuItem", new { @class = "nav-link" })</li>




                            <li>@Html.ActionLink("Login", "LoginPage", "SinglemenuItem", new { @class = "nav-link" })</li>
                            <li>@Html.ActionLink("Register", "CreateAccount", "SinglemenuItem", new { @class = "nav-link" })</li>

                            @if (string.IsNullOrEmpty(Session["uname"] as String))
                            {

                            }
                            else
                            {
                                <li>@Html.ActionLink("My Cart", "ViewUserCart", "SinglemenuItem", new { @class = "nav-link" })</li>
                                <li>@Html.ActionLink("My Orders", "LoadMyorders", "SinglemenuItem", new { @class = "nav-link" })</li>
                                <li>@Html.ActionLink("Edit Profile", "LoadUserEdit", "SinglemenuItem", new { @class = "nav-link" })</li>
                                <li>@Html.ActionLink("Logout", "UserLogout", "SinglemenuItem", new { @class = "nav-link" })</li>
                            }


                        </ul>

                    </nav>

                </div>
            </div>
        </header>
        <div>
            @RenderBody()
        </div>
        <footer id="footer" class="padding-50 dark">
            <div class="container">
                <div class="row">
                    <!-- Our location !-->
                    <div class="col-md-3 col-sm-6 col-xs-12 our_location">
                        <h3>Our Location</h3>
                        <p>Majesty  Head Office:</p>
                        <span>
                            63/A Anagarika Dharmapala Road
                            Matara.
                        </span>
                        <p class="mt30">Call for Reservations:<span>(71) 911-4567</span></p>
                        <p>E-mail: <span>admin@e-burger.com</span> </p>
                        <ul class="social mt30">
                            <li><a href="#" data-toggle="tooltip" title="Facebook"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="#" data-toggle="tooltip" title="Twitter"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="#" data-toggle="tooltip" title="Google+"><i class="fa fa-google-plus"></i></a></li>
