    <div id="content" class="main-content">
        <div class="layout-px-spacing">

            <div class="row layout-top-spacing">






                <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12 layout-spacing">
                    <div class="widget widget-table-two">

                        <div class="widget-heading">
                            <h5 class="">Orders Details</h5>
                            <h5 class="">Charger LKR 200 for delivery</h5>
                        </div>

                        <div class="widget-content">
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th><div class="th-content">Item Name</div></th>
                                            <th><div class="th-content">Meal Type</div></th>
                                            <th><div class="th-content">Meal Suitable</div></th>
                                            <th><div class="th-content">Size</div></th>
                                            <th><div class="th-content">Unit Price</div></th>
                                            <th><div class="th-content">qty</div></th>
                                            <th><div class="th-content">Id Genarated</div></th>
                                            
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var doc in Model)
                                        {
                                            <tr>
                                                <td><div class="td-content customer-name"><span>@doc.Itemname</span></div></td>
                                                <td><div class="td-content product-brand text-primary">@doc.Mealtype</div></td>
                                                <td><div class="td-content product-brand text-primary">@doc.Mealsuitable</div></td>
                                                <td><div class="td-content product-brand text-primary">@doc.Sizez</div></td>
                                                <td><div class="td-content product-brand text-primary">LKR @doc.Unitprice</div></td>
                                                <td><div class="td-content product-brand text-primary"> @doc.Qty</div></td>
                                                <td><div class="td-content product-brand text-primary"> @doc.Orderidgenarated</div></td>
                                                
                                                
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>






            </div>

